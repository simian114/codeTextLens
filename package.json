{
	"name": "code-text-lens",
	"displayName": "codeTextLens",
	"description": "codeTextLens",
	"version": "0.0.8",
	"repository": {
		"type": "git",
		"url": "https://github.com/simian114/codeTextLens"
	},
	"author": {
		"name": "sanam",
		"email": "simian114@gmail.com",
		"url": "https://github.com/simian114"
	},
	"publisher": "namSSang",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:code-lens.enableCodeLens",
		"onCommand:code-lens.disableCodeLens",
		"onCommand:code-lens.toggleCodeLens",
		"onCommand:code-lens.codelensAction",
		"onCommand:strapi-handler"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"title": "Enable CodeLens",
				"command": "code-lens.enableCodeLens",
				"category": "codeText"
			},
			{
				"title": "Disable Codelens",
				"command": "code-lens.disableCodeLens",
				"category": "codeText"
			},
			{
				"title": "toggle Codelens",
				"command": "code-lens.toggleCodeLens",
				"category": "codeText"
			},
			{
				"title": "strapi 추가 삭제 및 동기화",
				"command": "strapi-handler",
				"category": "strapi 조작"
			}
		],
		"configuration": {
			"title": "CodeTextLens",
			"properties": {
				"CodeTextLens.enabled": {
					"type": "boolean",
					"default": true,
					"description": "코드렌즈 기능 사용"
				},
				"CodeTextLens.regex": {
					"type": "string",
					"default": "\\'\\w+\\.\\w+\\-*\\w+\\.\\w+\\'",
					"markdownDescription": "codeText 파싱에 사용할 정규식을 입력해주세요.\n기본"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@faker-js/faker": "^6.3.1",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.8",
		"typescript": "^4.6.4",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	}
}